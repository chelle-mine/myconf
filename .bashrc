# Prompt
PS1="[\t]\W :. "
export PS1

# Default editor vim
export EDITOR=vim

# Add RVM to PATH
# export PATH="$PATH:$HOME/.rvm/bin"

# Enable colors
export CLICOLOR=1

# Aliases
alias la="ls -a"
alias ll="ls -l"
alias config="/usr/bin/git --git-dir=/Users/michelle/.cfg/ --work-tree=/Users/michelle"
